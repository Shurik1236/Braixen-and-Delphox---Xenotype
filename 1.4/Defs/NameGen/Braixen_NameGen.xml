<?xml version="1.0" encoding="utf-8" ?>
<Defs>
<RulePackDef>
        <defName>BraixenNameM</defName>
        <rulePack>
            <rulesStrings>
                <li>name->[FirstM] '[SecondM]' [Ident]</li>
            </rulesStrings>
            <rulesRaw>
                <li Class="Rule_File">
                    <keyword>FirstM</keyword>
                    <path>BraixenNames/BraixenMaleFirstName</path>
                </li>
                <li Class="Rule_File">
                    <keyword>SecondM</keyword>
                    <path>BraixenNames/BraixenMaleMidName</path>
                </li>
                <li Class="Rule_File">
                    <keyword>Ident</keyword>
                    <path>BraixenNames/BraixenSurName</path>
                </li>
            </rulesRaw>
        </rulePack>
    </RulePackDef>
    
