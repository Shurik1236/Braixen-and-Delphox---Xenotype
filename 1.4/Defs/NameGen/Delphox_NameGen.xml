<?xml version="1.0" encoding="utf-8" ?>
<Defs>
<RulePackDef>
        <defName>DelphoxNameM</defName>
        <rulePack>
            <rulesStrings>
                <li>name->[FirstM] '[SecondM]' [Ident]</li>
            </rulesStrings>
            <rulesRaw>
                <li Class="Rule_File">
                    <keyword>FirstM</keyword>
                    <path>DelphoxNames/DelphoxMaleFirstName</path>
                </li>
                <li Class="Rule_File">
                    <keyword>SecondM</keyword>
                    <path>DelphoxNames/DelphoxMaleMidName</path>
                </li>
                <li Class="Rule_File">
                    <keyword>Ident</keyword>
                    <path>DelphoxNames/DelphoxSurName</path>
                </li>
            </rulesRaw>
        </rulePack>
    </RulePackDef>
    
